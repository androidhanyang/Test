<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.hy.myfuck.ContentActivity">

    <RelativeLayout
        android:id="@+id/content_rl"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@color/qianred">

        <ImageView
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignTop="@+id/content_iv"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:src="@drawable/weibotable_more_tools_left" />

        <ImageView
            android:id="@+id/content_iv"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="73dp"
            android:layout_marginRight="73dp"
            android:src="@drawable/fenxiang" />
    </RelativeLayout>

    <WebView
android:layout_below="@+id/content_rl"
        android:id="@+id/webView"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</RelativeLayout>
